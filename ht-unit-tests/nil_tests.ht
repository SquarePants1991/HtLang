string nilString = nil;
Map nilMap = nil;
Array nilArray = nil;
Array nilDeclare;

assert("nilDeclare 需要等于 nil", nilDeclare == nil);
assert("nilString 需要等于 nil", nilString == nil);
assert("nilMap 需要等于 nil", nilMap == nil);
assert("nilArray 需要等于 nil", nilArray == nil);

nilString = "Hello World";
assert("nilString 需要不等于 nil", nilString != nil);

nilMap = {"Key": "Value"};
assert("nilMap 需要不等于 nil", nilMap != nil);

nilArray = ["123123", 1, 2];
assert("nilArray 需要不等于 nil", nilArray != nil);